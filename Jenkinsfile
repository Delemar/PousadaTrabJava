pipeline {
  agent any
  stages {
    stage('tetete') {
      agent any
      environment {
        reerre = '4'
      }
      steps {
        mail(subject: 'teste', body: 'teataetatae', to: 'delemar.neto@autoglass.com.br')
        echo 'sucesso'
        waitUntil() {
          sh 'f r t retrq r'
        }

      }
    }
    stage('tatatat') {
      parallel {
        stage('tatatat') {
          steps {
            bat 'tetetetet'
          }
        }
        stage('') {
          steps {
            sh 'ertr et'
            waitUntil() {
              sh 'wetr t e'
            }

          }
        }
      }
    }
  }
}